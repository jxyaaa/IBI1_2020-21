import matplotlib.pyplot as plt
labels='USA','India','Brazil','Russia','UK'
#each country
sizes=[29862124,11285561,11205972,4360823,4234924]
#total cases of each country
explode=[0,0,0,0,0]
plt.pie(sizes,labels=labels,explode=explode,shadow=True,autopct='%1.1f%%')
#draw a pie chart
plt.title('covid-19')
plt.show()
